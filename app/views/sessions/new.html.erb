<% provide(:title,"登录") %>
<h1 class="center">登录</h1>

<div class="row">
	<div class="col-md-6 col-md-offset-3 " >
		<%= bootstrap_form_tag url:sessions_path do |f| %>
			<%= f.email_field :email,label:"邮件" %>
			<%= f.password_field :password ,label:"密码"%>
			<%= f.submit "登录",class:"btn btn-default btn-primary" %>
		<% end %>
		<p>New user? <%= link_to "Sign up now!" ,signup_path %></p>
	</div>

</div>
